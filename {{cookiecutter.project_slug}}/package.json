{
  "name": "{{cookiecutter.project_slug}}",
  "dependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.0",
    "@babel/plugin-transform-runtime": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.4.2",
    "@fortawesome/fontawesome-free": "5.7.2",
    "acorn": "6.1.0",
    "adhocracy4": "liqd/adhocracy4#698cdeba66e10ba204866c93f31c115c50a2790e",
    "babel-loader": "8.0.5",
    "bootstrap": "4.3.1",
    "copy-webpack-plugin": "5.0.2",
    "css-loader": "2.1.1",
    "datepicker": "git+https://github.com/liqd/datePicker.git",
    "feature-detect": "1.0.0",
    "file-loader": "3.0.1",
    "file-saver": "2.0.1",
    "immutability-helper": "3.0.0",
    {% if cookiecutter.add_maps_and_mapideas_app == 'y' %}
    "leaflet": "1.3.4",
    "leaflet-draw": "1.0.4",
    "leaflet.markercluster": "1.4.1",
    {% endif %}
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "popper.js": "1.14.7",
    "react-flip-move": "3.0.3",
    "react-sticky-box": "0.8.0",
    "react-bootstrap-typeahead": "3.3.4",
    "sass-loader": "7.1.0",
    "sass-planifolia": "0.6.0",
    "select2": "4.0.5",
    "shpjs": "3.4.3",
    "tether": "1.4.5",
    "timeago.js": "3.0.2",
    "webpack": "4.29.6",
    "webpack-merge": "4.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.14.1",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "husky": "1.3.1",
    "markdownlint-cli": "0.14.1",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-declaration-strict-value": "1.1.2",
    "webpack-cli": "3.2.3"
  },
  "scripts": {
    "build:prod": "webpack --config webpack.prod.js --mode production",
    "build": "webpack --config webpack.dev.js --mode development",
    "watch": "webpack --config webpack.dev.js --watch --mode development",
    "lint": "eslint '{apps,cms,{{cookiecutter.project_slug}}}/**/*.{js,jsx}' && stylelint '{{cookiecutter.project_slug}}/assets/scss/**/*.scss' --syntax scss"
  },
  "browserslist": "last 3 versions, ie >= 10",
  "husky": {
    "hooks": {
      "pre-commit": "make lint"
    }
  }
}
